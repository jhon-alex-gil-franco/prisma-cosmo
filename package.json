{
  "name": "cosmo",
  "version": "0.0.1",
  "description": "Prisma GraphQL generator",
  "main":"./lib/index.js",
  "author": "prevalentWare",
  "license": "MIT",
  "keywords": [
    "prisma",
    "graphql",
    "sdl-first"
  ],
  "devDependencies": {
    "@prisma/client": "^3.9.2",
    "@types/node": "^17.0.19",
    "apollo-server-micro": "^3.6.3",
    "graphql": "^16.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "create": "npm run build && npm run test",
    "prettier-format": "prettier --config .prettierrc prisma/generated/**/*.ts --write",
    "start": "ts-node src/index.ts && prettier --config .prettierrc prisma/generated/**/*.ts --write",
    "build": "tsc -p ."
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
}
